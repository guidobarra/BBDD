public class GraphTest {

	@Test
	public void testMinimumSwaps() {
//		int[] array = {1, 5, 4, 3, 2};
		int[] array = {4, 3, 1, 2 };
		int minimum = Graph.minimumSwaps(array);
		System.out.println(minimum);
	}

	@Test
