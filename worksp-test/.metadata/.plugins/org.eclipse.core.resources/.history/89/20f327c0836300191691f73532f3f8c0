//		int[] array =  {3,1,2,4};
//		int[] array = {1, 5, 4, 3, 2};
		int[] array = {4, 3, 1, 2 };
		int minimum = Graph.minimumSwapsSimple(array);
		System.out.println(minimum);
	}
	

}
